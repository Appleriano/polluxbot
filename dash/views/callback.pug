        /****** KEYFRAME ANIMATIONS ******/
        @keyframes quick-glow {
          15% {
            z-index: 99;
            background: #FF2995;
            box-shadow: 0 0 10px 2px #FF2995;
          }
        }
        @keyframes glow {
          50% {
            z-index: 99;
            background: #FF2995;
            box-shadow: 0 0 10px 2px #FF2995;
          }
        }
        @-webkit-keyframes quick-glow {
          15% {
            z-index: 99;
            background: #FF2995;
            box-shadow: 0 0 10px 2px #FF2995;
          }
        }
        @-webkit-keyframes glow {
          50% {
            z-index: 99;
            background: #FF2995;
            box-shadow: 0 0 10px 2px #FF2995;
          }
        }
        .loadere{
          position:absolute;
          bottom:0;
          height:16px;
          width:100%;
          background: #381928

        }
        .bare{
          position:absolute;
          bottom:0;
          height:16px;
          width:100%;
          background: #FF2995;
          animation: thisload 3s both;
        }
        .bareb{
          position:relative;
          bottom:0;
          height:16px;
          width:100%;
          box-shadow: 0 0 15px 2px #FF2995;
          animation: glower .6s both infinite;
        }
         @keyframes glower {
            50% {box-shadow: 0 0 0px 0px #FF2995;}
            }
          @keyframes thisload {
            0% {
            transform:scale(0,1);
            transform:translateX(-100%);
            }
            100% {transform:scale(1,1);}
            }

