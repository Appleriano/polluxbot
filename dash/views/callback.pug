        .container {
          position: absolute;
          margin: auto;
          left: 0;
          right: 0;
          width: 60%;
          height: 100%;
          display: flex;
          display: -webkit-flex;
          flex-wrap: wrap;
          -webkit-flex-wrap: wrap;
          justify-content: space-around;
          -webkit-justify-content: space-around;
          align-items: center;
          -webkit-align-items: center;
          align-content: center;
          -webkit-align-content: center;
        }

        .title {
          position: absolute;
          margin: auto;
          left: 0;
          right: 0;
          top: auto;
          bottom: 10px;
          /* background:pink; */
          width: 190px;
          height: 20px;
          font-family: Sans-serif;
          font-weight: 100;
          color: #8f0048;
        }

        .grid-big {
          display: flex;
          flex-wrap: wrap;
          -webkit-flex-wrap: wrap;
          justify-content: center;
          -webkit-justify-content: center;
          align-items: center;
          -webkit-align-items: center;
          align-content: center;
          -webkit-align-content: center;
          width: 70px;
          height: 70px;
        }

        .grid-mini {
          position: absolute;
          margin: auto;
          left: 0;
          right: 0px;
          top: auto;
          bottom: 5px;
          display: flex;
          flex-wrap: wrap;
          -webkit-flex-wrap: wrap;
          justify-content: center;
          -webkit-justify-content: center;
          align-items: center;
          -webkit-align-items: center;
          align-content: center;
          -webkit-align-content: center;
          width: 25px;
          height: 35px;
        }

